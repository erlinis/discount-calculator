{
  "name": "discount-calculator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.11",
    "package-lock.json": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.0.4"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "yarn build:css -w",
    "start": "NODE_ENV=development concurrently \"yarn watch:css\" \"react-scripts start\"",
    "build": "NODE_ENV=production yarn build:css && react-scripts build",
    "postbuild": "surge ./build --domain https://discount-calculator.surge.sh/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@tailwindcss/jit": "^0.1.3",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.0",
    "cssnano": "^4.1.7",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-uncss": "^0.17.0",
    "source-map-explorer": "^2.5.2",
    "surge": "^0.22.1",
    "uncss": "^0.17.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "volta": {
    "node": "14.16.0",
    "yarn": "1.22.10"
  }
}
